  },
  "repository": {
      "type": "git",
      "url": "git+https://github.com/DennisSchulmeister/lernspiel.online.git"
  },
  "type": "module",
  "devDependencies": {
    "ava": "^6.1.3",
    "nodemon": "^3.1.4"
